    "#combining\n",
    "df_tc=pd.concat([pos_df_tc,neg_df_tc],ignore_index=True)\n",
    "# shuffling rows\n",
    "df_tc = df_tc.sample(frac=1).reset_index(drop=True)\n",
    "print(df_tc.shape)  # perfectly fine.\n",
    "df_tc.head()"
