    "# Use nltk's English stopwords.\n",
    "stopwords = nltk.corpus.stopwords.words('english')\n",
    "stopwords.append(\"'s\")\n",
    "stopwords.append(\"'m\")\n",
    "stopwords.append(\"n't\")\n",
    "stopwords.append(\"br\")\n",
    "stopwords.append(\"'d\")\n",
